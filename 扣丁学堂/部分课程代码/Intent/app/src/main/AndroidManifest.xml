<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.choubao.www.intent">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Main2Activity">
            <intent-filter android:priority="-1">
                <action android:name="com.choubao.action.MY_ACTION" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Main3Activity">
            <intent-filter android:priority="-1">
                <category android:name="com.choubao.action.MY_CATEGROY" />

                <action android:name="com.choubao.action.MY_ACTION" />
                <action android:name="com.choubao.action.MY_ACTION2" />
                <action android:name="com.choubao.action.MY_ACTION3" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Main4Activity">
            <intent-filter>

                <!-- <action android:name="com.choubao.action.MY_ACTION" /> -->
                <action android:name="com.choubao.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />

                <data
                    android:host="www.baidu.com"
                    android:mimeType="text/html"
                    android:scheme="http" />
            </intent-filter>
        </activity>
        <activity android:name=".MainAActivity" />
        <activity
            android:name=".MainBActivity"
            android:launchMode="standard" />
        <activity android:name=".Main5Activity" />
        <activity android:name=".Main6Activity"></activity>
    </application>

</manifest>